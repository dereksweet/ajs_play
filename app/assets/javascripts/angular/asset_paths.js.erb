var asset_paths = { };

<%
  templates = Dir.glob('app/assets/templates/**/*').select { |f| File.file?(f) }
  templates.each do |template|
    relative_template = template.gsub('app/assets/templates/', '').gsub('.erb', '')
%>
    asset_paths['<%= relative_template %>'] = '<%= asset_path(relative_template) %>';
<%
  end
%>
